<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="fx_post_pane" prefHeight="488.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PostsControl">
   <children>
      <Label alignment="CENTER" layoutY="16.0" prefHeight="29.0" prefWidth="430.0" text="帖子列表">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ListView fx:id="fx_post_listview" layoutY="45.0" prefHeight="412.0" prefWidth="430.0" />
      <Button fx:id="fx_post_del_btn" layoutY="455.0" mnemonicParsing="false" onAction="#onBtnDelete" prefHeight="29.0" prefWidth="215.0" text="删除帖子">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="fx_post_update_btn" layoutX="215.0" layoutY="455.0" mnemonicParsing="false" onAction="#onBtnUpdate" prefHeight="23.0" prefWidth="215.0" text="查看修改">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
